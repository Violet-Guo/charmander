{
    "id":          "taskresolver",
    "dockerimage": "taskresolver",
    "mem":         "20",
    "sla":         "one-per-node",
    "notmetered":   true,
    "volumes": [
        {
            "host_path":"/var/run/docker.sock",
            "container_path":"/var/run/docker.sock",
            "mode":"ro"
        },
        {
            "host_path":"/usr/bin/docker",
            "container_path":"/usr/bin/docker",
            "mode":"ro"
        }
    ],
    "ports": [
        {
            "host_port":"31900",
            "container_port":"9000"
        }
    ]
}
