{
    "id":          "influxdb",
    "dockerimage": "influxdb",
    "mem":         "200",
    "cpus":        "1",
    "nodetype":    "analytics",
    "notmetered":  true,
    "volumes": [
        {
            "host_path":"/analytics/log/influxdb",
            "container_path":"/log/",
            "mode":"rw"
        }
    ],
    "ports": [
        {
            "host_port":"31400",
            "container_port":"8083"
        },
        {
            "host_port":"31410",
            "container_port":"8086"
        }
    ]

}
